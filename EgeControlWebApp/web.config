<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
    </handlers>
    <aspNetCore 
      processPath=".\EgeControlWebApp.exe" 
      arguments="" 
      stdoutLogEnabled="true" 
      stdoutLogFile=".\logs\stdout" 
      hostingModel="outofprocess" />
    
    <!-- Error handling for startup issues -->
    <httpErrors>
      <remove statusCode="500" subStatusCode="30" />
      <error statusCode="500" subStatusCode="30" responseMode="File" path="500-30-error.html" />
    </httpErrors>
  </system.webServer>
</configuration>
